{
  "name": "mush-api",
  "version": "0.1.0",
  "description": "Typescript API for mushroom farm management",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "tsc --project tsconfig.json",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify -r dotenv/config src/index.ts",
    "start": "node -r dotenv/config dist/src/index.js",
    "test": "node_modules/.bin/jest --detectOpenHandles --forceExit --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PauloJMF/mush-api.git"
  },
  "keywords": [
    "nodejs",
    "api",
    "typescript"
  ],
  "author": "Paulo Machado",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PauloJMF/mush-api/issues"
  },
  "homepage": "https://github.com/PauloJMF/mush-api#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.2",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.1",
    "prisma": "^3.15.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1"
  }
}
